<p id="notice"><%= notice %></p>

<h1>Delegations</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Tel</th>
      <th>Email</th>
      <th>Cp</th>
      <th>Provincia</th>
      <th>Municipio</th>
      <th>Comuna</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delegations.each do |delegation| %>
      <tr>
        <td><%= delegation.nome %></td>
        <td><%= delegation.endereco %></td>
        <td><%= delegation.tel %></td>
        <td><%= delegation.email %></td>
        <td><%= delegation.cp %></td>
        <td><%= delegation.provincia %></td>
        <td><%= delegation.municipio %></td>
        <td><%= delegation.comuna %></td>
        <td><%= delegation.status %></td>
        <td><%= link_to 'Show', delegation %></td>
        <td><%= link_to 'Edit', edit_delegation_path(delegation) %></td>
        <td><%= link_to 'Destroy', delegation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delegation', new_delegation_path %>
