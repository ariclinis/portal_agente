<div class="main-content">
    <section class="section">
      <div class="section-header">
        <h1>Desembarca</h1>
      </div>
  <p id="notice"><%= notice %></p>

<table class="table">
  <thead>
    <tr>
      <th>Embarkation</th>
      <th>N movimento</th>
      <th>Porto origem</th>
      <th>Porto destino</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @desembarkations.each do |desembarkation| %>
      <tr>
        <td><%= desembarkation.embarkation_id %></td>
        <td><%= desembarkation.n_movimento %></td>
        <td><%= desembarkation.porto_origem %></td>
        <td><%= desembarkation.porto_destino %></td>
        <td><%= desembarkation.status %></td>
        <td><%= link_to 'Desatracar', "/desatracar/#{desembarkation.embarkation_id}"%></td>
        <td><%= link_to 'Ver', desembarkation %></td>
         <td><%= link_to 'Eliminar', desembarkation, method: :delete, data: { confirm: 'Tem certeza ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</section>

</div>
<br>

